def generate_series(a):
    result = []
    for i in range(1, a+1):
        if i % 2 != 0:
            result.append(i)
    return result

print(generate_series(1))   # Output: [1]
print(generate_series(2))   # Output: [1, 3]
print(generate_series(3))   # Output: [1, 3, 5]
print(generate_series(4))   # Output: [1, 3, 5, 7]
